{
    "name": "fa-constants",
    "version": "0.1.0",
    "description": "A list of all font awesome icons as js object. Font Awesome by Dave Gandy - http://fontawesome.io",
    "main": "./dist/index.js",
    "scripts": {
        "buildall": "npm run parse && npm run test && npm run build",
        "parse": "node parse.js",
        "start": "node ./dist/index.js",
        "build": "node build.js",
        "prepublish": "npm run build",
        "test": "npm run lint && mocha --timeout 15000 --compilers js:babel-core/register --recursive \"tests/*.tests.js\"  \"lib/*.js\"",
        "test:watch": "npm test -- --watch",
        "lint": "eslint -c ./.eslintrc.json ./lib",
        "lint-fix": "eslint --fix -c ./.eslintrc.json ./lib"
    },
    "keywords": [
        "fontawesome"
    ],
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "author": "Jan KÃ¼ster <jkuester@uni-bremen.de>",
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chai": "^4.1.2",
        "eslint": "^4.16.0",
        "eslint-plugin-compat": "^2.2.0",
        "eslint-plugin-import": "^2.8.0",
        "js-yaml": "^3.10.0",
        "mocha": "^5.0.0"
    }
}
