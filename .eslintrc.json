{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
    },
    "plugins": [
        "compat"
    ],
    "rules": {
        "compat/compat": "error"
    },
    "parserOptions": {
        "sourceType": "module"
    }
}